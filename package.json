{
  "name": "todone",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/file-saver": "^2.0.1",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "axios": "^0.21.1",
    "file-saver": "^2.0.2",
    "localforage": "^1.7.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-ga": "^3.0.0",
    "react-markdown": "^5.0.2",
    "react-scripts": "4.0.1",
    "remark-gfm": "^1.0.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "npm run test -- --coverage --watchAll=false",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.8.0",
    "@testing-library/react": "^11.1.0",
    "codecov": "^3.7.1",
    "husky": "^4.3.6",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "coverageReporters": [
      "html",
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.(ts|tsx)",
      "!src/index.tsx",
      "!src/test-utils/**",
      "!src/react-app-env.d.ts",
      "!src/models/**.ts"
    ]
  }
}
